* {
    color: #ffffff;
    font-family: Arial;
    font-size: 16px;
}

QDialog {
    background: #000000;
}

QPushButton,
QComboBox,
QComboBox QAbstractItemView {
    font-size: 14px;
    border-radius: 5px;
    background: #69B0E4;
    padding: 5px;
}

QAbstractItemView::item:hover {
    background-color: #4B91CD;
    border: 0;
}

QPushButton:hover {
    background: #8CB9E1;
}

QPushButton:pressed {
    background: #4B91CD;
}